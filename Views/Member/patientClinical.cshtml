<!-------------------------------------------------------------------------------------- html-body ------------------------------------------------------------------------------------>
<div class="dashboardContainer zero-padding">
    <div class="patientDashboard_View">
        <!-- breadcrumbs -->
        <ul class="breadcrumbs_patientDashboard">
            <li style="font-size: 12px; color: #0081b3; font-weight: 200">You are here:</li>
            <li><a href="@Url.Action("Index", "Home", new {})">Home</a></li>
            <li><a href="@Url.Action("patientDashboard", "Member", new { DependentID = Context.Request.Query["DependentID"], pro = Context.Request.Query["pro"]})">Patient</a></li>
            <li><a href="#">@ViewBag.Title</a></li>
        </ul>
        <!-- navigation-collection -->
        <div class="navCollection_patientDashboard">
            @Html.Partial("_navCollection")
        </div>
        <!-- left-navigation-panel -->
        @Html.Partial("_navSlider", new { DependentID = Context.Request.Query["DependentID"], pro = Context.Request.Query["pro"] })
        <!--profile-information-->
        <div class="profileBar">
            @Html.RenderAction("_profileBar_edit", "Member", new { DependentID = Context.Request.Query["DependentID"], pro = Context.Request.Query["pro"] })
        </div>
    </div>

    <div class="profilebar-spacer"></div>

    <div class="patientDashboard_container">
        <h4 class="summaryHeading">Patient clinical</h4>
        <hr class="patientProfile_Ruler" />
        <div class="container-tab">
            <div class="row">
                <div class="hidden-fields">
                    <input type="hidden" name="dependantID" id="dependantID" value="@TempData["dependantID"].ToString()" />
                    <input type="hidden" name="program" id="program" value="@TempData["program"].ToString()" />
                </div>
                <div class="span12">
                    <div class="container-tab">
                        <!------------------------------------------------------------------------------------ tab-navigation ----------------------------------------------------------------------------------->
                        <ul class="nav nav-pills nav-justified" role="tablist">
                            <li role="presentation" class="tab-clinical active"><a href="#summary" aria-controls="summary" class="tab-label" data-toggle="tab">Summary</a></li>
                            <li role="presentation" class="tab-clinical"><a href="#pathology" aria-controls="pathology" class="tab-label" role="tab" data-toggle="tab" id="pathology_tab">Pathology</a></li>
                            <li role="presentation" class="tab-clinical"><a href="#script" aria-controls="script" class="tab-label" role="tab" data-toggle="tab" id="script_tab">Script</a></li>
                            <li role="presentation" class="tab-clinical"><a href="#patientHistory" aria-controls="patientHistory" class="tab-label" role="tab" data-toggle="tab" id="patient_history_tab">Patient history</a></li>
                            <li role="presentation" class="tab-clinical"><a href="#scriptHistory" aria-controls="scriptHistory" class="tab-label" role="tab" data-toggle="tab" id="script_history_tab">Script history</a></li>
                        </ul>
                        <!-------------------------------------------------------------------------------------- tab-panels ------------------------------------------------------------------------------------->
                        <div class="tab-content">
                            <!---------------------------------------------------------------------------------- tab-summary ------------------------------------------------------------------------------------>
                            <div role="tabpanel" class="tab-pane active" id="summary">
                                <div class="row">
                                    <br />
                                    <!------ pathology details ------>
                                    <div class="clinical-summary">
                                        <h4 class="subHeading">Pathology details</h4>
                                        <hr class="patientProfile_Ruler" />
                                        <div class="row">
                                            <table class="table patient-clinical-table" id="pathology_details" style="margin-bottom: 1.5%!important">
                                                <thead>
                                                    <tr>
                                                        <th style="width: 25%">
                                                            @Html.DisplayName("Next pathology due")
                                                        </th>
                                                        <th style="width: 25%">
                                                            @Html.DisplayName("Last pathology")
                                                        </th>
                                                        <th onclick="sortTable(0)" style="width: 25%">
                                                            @Html.DisplayName("Outstanding") <i class="fa fa-fw fa-sort"></i>
                                                        </th>
                                                        <th onclick="sortTable(0)" style="width: 25%; text-align: center!important">
                                                            @Html.DisplayName("Pathology due")
                                                        </th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr>
                                                        <td class="patient-clinical-pathology-details">
                                                            @Html.DisplayFor(model => model.nextPathologyDate)
                                                        </td>
                                                        <td class="patient-clinical-pathology-details">
                                                            @Html.DisplayFor(model => model.lastPathologyDate)
                                                        </td>
                                                        <td class="patient-clinical-pathology-details">
                                                            @Html.DisplayFor(model => model.pathologyBehind) days
                                                        </td>
                                                        <td class="patient-clinical-check">
                                                            @if (Model.isPathologyDue)
                                                            {
                                                                <img src="~/Content/Images/Navigation/5. Reports/reportCheck.png" />
                                                            }
                                                            else
                                                            {
                                                                <img src="~/Content/Images/Navigation/5. Reports/reportCheck_x.png" />
                                                            }
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                    <!------ doctor-referral - hcare-1136 ------>
                                    <div class="clinical-summary">
                                            <h4 class="subHeading">
                                                Doctor referral
                                                <a class="create-new" href="@Url.Action("DoctorReferral_Create", "Member", new { DependentID = Model.DependentID, pro=Context.Request.Query["pro"] })" style="font-size: 14px">Create new <span><i class="fa fa-plus-circle" style="font-size: 12px"></i></span></a>
                                            </h4>
                                            <hr class="patientProfile_Ruler" />
                                            <div class="row">
                                                <table class="table patient-clinical-table sortable" id="doctor-referral" style="margin-bottom: 1.5%!important">
                                                    <thead>
                                                        <tr>
                                                            <th style="width: 15%; text-align: center!important">@Html.DisplayNameFor(model => model.DoctorReferrals[0].CreatedDate) <i class="fa fa-fw fa-sort"></i></th>
                                                            <th style="width: 15%; text-align: center!important">@Html.DisplayNameFor(model => model.DoctorReferrals[0].ReferralDate) <i class="fa fa-fw fa-sort"></i></th>
                                                            <th style="width: 45%">@Html.DisplayNameFor(model => model.DoctorReferrals[0].Comment) <i class="fa fa-fw fa-sort"></i></th>
                                                            <th style="width: 10%; text-align: center!important">@Html.DisplayNameFor(model => model.DoctorReferrals[0].Program) <i class="fa fa-fw fa-sort"></i></th>
                                                            <th style="width: 10%; text-align: center!important">@Html.DisplayName("Action")</th>
                                                        </tr>
                                                    </thead>
                                                    @if (Model.DoctorReferrals.Count() == 0)
                                                    {
                                                        <tr><td colspan="5" style="text-align: center!important; color: rgba(128, 128, 128, 0.50)!important; font-size: 18px!important"><span><i class="fas fa-info-circle" style="color: rgba(128, 128, 128, 0.50)"></i></span> no records found</td></tr>
                                                    }
                                                    else
                                                    {
                                                        foreach (var item in Model.DoctorReferrals)
                                                        {
                                                            <tbody>

                                                                @if (item.FollowUp == true)
                                                                {
                                                                    <tr>
                                                                        <td colspan="4" style="text-align: center!important; color: rgba(204, 0, 0, 0.70)!important; font-size: 18px!important"><span><i class="fas fa-info-circle" style="font-size: 18px; color: rgba(204, 0, 0, 0.70)!important"></i></span> Follow up required!</td>
                                                                        <td class="text-center">
                                                                            <input type="submit" class="link-button" value="View" onclick="location.href='@Url.Action("DoctorReferral_Details", "Member", new { id = item.Id, pro = Context.Request.Query["pro"] })'" /> /
                                                                            <input type="submit" class="link-button" value="Edit" onclick="location.href='@Url.Action("DoctorReferral_Edit", "Member", new { id = item.Id, pro = Context.Request.Query["pro"] })'" />
                                                                        </td>
                                                                    </tr>
                                                                }
                                                                else
                                                                {
                                                                    <tr>
                                                                        <td class="text-center" data-dateformat="DD-MM-YYYY">@Html.DisplayFor(modelItem => item.CreatedDate)</td>
                                                                        <td class="text-center" data-dateformat="DD-MM-YYYY">@Html.DisplayFor(modelItem => item.ReferralDate)</td>
                                                                        <td>@Html.DisplayFor(modelItem => item.Comment)</td>
                                                                        <td class="text-center">@Html.DisplayFor(modelItem => item.Program)</td>
                                                                        <td class="text-center">
                                                                            <input type="submit" class="link-button" value="View" onclick="location.href='@Url.Action("DoctorReferral_Details", "Member", new { id = item.Id, pro = Context.Request.Query["pro"] })'" /> /
                                                                            <input type="submit" class="link-button" value="Edit" onclick="location.href='@Url.Action("DoctorReferral_Edit", "Member", new { id = item.Id, pro = Context.Request.Query["pro"] })'" />
                                                                        </td>
                                                                    </tr>
                                                                }
                                                            </tbody>
                                                        }
                                                    }
                                                </table>
                                            </div>
                                        </div>
                                    <!------ clinical rules ------>
                                    <div class="clinical-summary">
                                        <h4 class="subHeading">Clinical rules</h4>
                                        <hr class="patientProfile_Ruler" />
                                        <div class="row">
                                            <table class="table patient-clinical-table sortable" id="clinical_rules" style="margin-bottom: 1.5%!important">
                                                <thead>
                                                    <tr>
                                                        <th style="width: 15%">
                                                            @Html.DisplayNameFor(model => model.rulesBroken[0].effectiveDate) <i class="fa fa-fw fa-sort"></i>
                                                        </th>
                                                        <th style="width: 30%">
                                                            @Html.DisplayNameFor(model => model.rulesBroken[0].ruleName) <i class="fa fa-fw fa-sort"></i>
                                                        </th>
                                                        <th onclick="sortTable(0)" style="width: 45%">
                                                            @Html.DisplayNameFor(model => model.rulesBroken[0].messages)
                                                        </th>
                                                    </tr>
                                                </thead>
                                                @if (Model.BrokenRules.Count() == 0)
                                                {
                                                    <tr><td colspan="3" style="text-align: center!important; color: rgba(128, 128, 128, 0.50)!important; font-size: 18px!important"><span><i class="fas fa-info-circle" style="color: rgba(128, 128, 128, 0.50)"></i></span> no records found</td></tr>
                                                }
                                                else
                                                {
                                                    foreach (var item in Model.BrokenRules)
                                                    {
                                                        <tbody>
                                                            <tr>
                                                                <td data-dateformat="DD-MM-YYYY">
                                                                    @Html.DisplayFor(modelItem => item.CreatedDate)
                                                                </td>
                                                                <td>
                                                                    @Html.DisplayFor(modelItem => item.RuleName)
                                                                </td>
                                                                <td>
                                                                    @Html.DisplayFor(modelItem => item.Message)
                                                                </td>
                                                                @*<td data-value="0">
                                                                        @foreach (var message in item.messages)
                                                                        {
                                                                            @Html.DisplayFor(messageItem => message.instruction)<br />
                                                                        }
                                                                    </td>*@
                                                            </tr>
                                                        </tbody>
                                                    }
                                                }

                                            </table>
                                        </div>
                                    </div>
                                    <!------ risk rating ------>
                                    <div class="clinical-summary">
                                        <div class="row">
                                            <h4 class="subHeading">
                                                Risk rating
                                                @if (User.HasRole("4. Advisor") || (User.HasRole("5. Super user")))
                                                {
                                                    <a class="create-new" href="@Url.Action("CreateRiskRating", "Member", new { DependentID = Model.DependentID, pro=Context.Request.Query["pro"] })" style="font-size: 14px">Create new <span><i class="fa fa-plus-circle" style="font-size: 12px"></i></span></a>
                                                }
                                            </h4>
                                            <hr class="patientProfile_Ruler" />
                                        </div>
                                        <div class="row">
                                            <table class="table patient-clinical-table sortable" id="risk_rating" style="margin-bottom: 1.5%!important">
                                                <thead>
                                                    <tr>
                                                        <th style="width: 15%">
                                                            @Html.DisplayName("Effective Date") <i class="fa fa-fw fa-sort"></i>
                                                        </th>
                                                        <th style="width: 30%">
                                                            @Html.DisplayName("Risk Rating") <i class="fa fa-fw fa-sort"></i>
                                                        </th>
                                                        <th onclick="sortTable(0)" style="width: 45%">
                                                            @Html.DisplayName("Reason")
                                                        </th>
                                                    </tr>
                                                </thead>
                                                @if (Model.RiskRating.Count() == 0)
                                                {
                                                    <tr><td colspan="3" style="text-align: center!important; color: rgba(128, 128, 128, 0.50)!important; font-size: 18px!important"><span><i class="fas fa-info-circle" style="color: rgba(128, 128, 128, 0.50)"></i></span> no records found</td></tr>
                                                }
                                                else
                                                {
                                                    foreach (var item in Model.RiskRating)
                                                    {
                                                        <tbody>
                                                            <tr>
                                                                <td data-dateformat="DD-MM-YYYY">
                                                                    @Html.DisplayFor(modelItem => item.effectiveDate)
                                                                </td>
                                                                <td>
                                                                    @Html.DisplayFor(modelItem => item.RiskId)
                                                                </td>
                                                                <td data-value="0">
                                                                    @Html.DisplayFor(modelItem => item.reason)
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    }
                                                }

                                            </table>
                                        </div>
                                    </div>
                                    <!-- collapse - hidden 10 January 2020-->
                                    <div class="row col-md-12 hidden">
                                        <div class="showCollapse">
                                            <a data-toggle="collapse" href="#clinicalCollapse" aria-expanded="false" input type="button" class="btn buttonRound_formCreate">Full Details</a>
                                        </div>

                                        <div class="collapse" id="clinicalCollapse">
                                            <div style="margin-bottom: 5%!important"></div>
                                            <div class="contentBlock_fullDetailsPC">
                                                <div class="row" style="text-align:center">

                                                    <h4 style="color: #003366; margin-top: 1%; margin-left: 2%; margin-bottom: 1%!important; font-size: 18px; font-family: 'Century Gothic'; font-weight: 300; font-style: normal;">Patient Treatment Details</h4>
                                                    <hr class="fancyruler">
                                                    <h4 style="color: #003366; margin-top: 0%; margin-left: 2%; margin-bottom: 0%!important; font-size: 16px; font-family: 'Century Gothic'; font-weight: 300; font-style: normal;">Last Order Details</h4>
                                                    <hr class="fancyruler">
                                                </div>
                                                <br />

                                                <!--Full Details / Last Order Details-->

                                                <div class="row col-md-12">
                                                    <div class="col-md-3">
                                                        Has Claimed Before
                                                    </div>
                                                    <div class="col-md-3">
                                                        unavailable
                                                    </div>
                                                    <div class="col-md-3">
                                                        Date Due for Reorder
                                                    </div>
                                                    <div class="col-md-3">
                                                        unavailable
                                                    </div>
                                                </div>

                                                <div class="row col-md-12">
                                                    <div class="col-md-3">
                                                        Last Claim Date
                                                    </div>
                                                    <div class="col-md-3">
                                                        unavailable
                                                    </div>
                                                    <div class="col-md-3">
                                                        Has Open Assignment
                                                    </div>
                                                    <div class="col-md-3">
                                                        yes/no
                                                    </div>
                                                </div>

                                                <div class="row col-md-12">
                                                    <div class="col-md-3">
                                                        Due for Reorder
                                                    </div>
                                                    <div class="col-md-3">
                                                        unavailable
                                                    </div>
                                                </div>
                                                <div class="row"></div>
                                                <br />
                                                <hr class="fancyruler">

                                                <!--Full Details / Script Details-->
                                                <div style="text-align:center">
                                                    <h4 style="color: #003366; margin-top: 0%; margin-left: 2%; margin-bottom: 0%!important; font-size: 16px; font-family: 'Century Gothic'; font-weight: 300; font-style: normal;">Script Details</h4>
                                                    <hr class="fancyruler">
                                                </div>
                                                <br />

                                                <div class="row col-md-12">
                                                    <div class="col-md-3">
                                                        Patient has a script
                                                    </div>
                                                    <div class="col-md-3">
                                                        @Html.DisplayFor(model => model.hasScript)
                                                    </div>
                                                    <div class="col-md-3">
                                                        Patient script outstanding
                                                    </div>
                                                    <div class="col-md-3">
                                                        @Html.DisplayFor(model => model.requiresNewScript)
                                                    </div>
                                                </div>

