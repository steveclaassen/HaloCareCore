                            <div class="col-md-4 disclaimer-checkbox">
                                @Html.CheckBox("depression-yes") <label for="depression-yes" title="yes"><span id="depression-yes" class="grey"><i class="fas fa-circle question-button"></i></span></label>
                            </div>
                        </td>
                        <td style="width: 2.5%">
