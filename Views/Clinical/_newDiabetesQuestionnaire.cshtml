                                                }, "-- Select --", new { @class = "infoBox_doctorQuestionnaire", @required = "required" })

                                                @Html.ValidationMessageFor(model => model.vision.vision, "", new { @class = "text-danger", @style = "font-size: 110%;margin-left: 1%" })

