@model HaloCareCore.Models.DoctorQuestionnaireViewModel
@{
    ViewBag.Title = "Diabetes Questionnaire";
    Layout = "~/Views/Shared/_LayoutModal.cshtml";
}

<comment>
    <!--
        25 May 2020 -- HCare-832(Questionnaire: Diabetes Questionnaire development)
        As per SR requirement as well as discussions held. A NEW Diabetes questionnaire has been developed.

        17 June 2020 -- HCare-902(Limit dates Diabetes questionnaire)
        Limited dates to 1900-01-01 on all three diabetes questionnaires
    -->
</comment>
<style>
    ::-webkit-input-placeholder { /* Chrome */
        color: #333;
        font-weight: 500;
        font-size: 14px;
        transition: opacity 250ms ease-in-out;
    }

    :focus::-webkit-input-placeholder {
        opacity: 0.5;
    }

    .fab {
        position: fixed;
        z-index: 100;
        right: 3.5%;
        top: 5.5%;
    }

    .fa-stack {
        font-size: 20px;
        color: rgba(255, 255, 255, 0.75)
    }

        .fa-stack:hover {
            transition-duration: 0.1s;
            font-size: 22px;
            color: orange;
        }

    i {
        vertical-align: middle;
        margin-left: 5px;
    }

    .field-validation-error:before {
        font-family: FontAwesome;
        content: "\f06a\00a0";
        animation: pulse 2s ease infinite;
    }

    .field-validation-valid {
        display: none;
    }

    .validation-summary-valid {
        display: none;
    }

    .subtext {
        color: #808080;
        font-size: 14px;
    }

    .select2-container--default .select2-results__option--highlighted[aria-selected] .subtext {
        color: #fff
    }

    .select2-container--default .select2-results__option--highlighted[aria-selected]:hover .subtext {
        color: #fff
    }

    .select2-container--default .select2-selection--single {
        display: block !important;
        width: 380px !important;
        max-height: 100px;
        height: 30px;
        margin-left: 10px;
        padding: 6px 6px;
        line-height: 1.428571429;
        color: #003366;
        border-radius: 0px !important;
        border: none !important;
        border-bottom: 1px solid #e8e6e6 !important;
        background-color: transparent !important;
        -webkit-box-shadow: none !important;
        box-shadow: none !important;
        outline: none !important
    }

        .select2-container--default .select2-selection--single .select2-selection__placeholder {
            color: white !important;
        }

        .select2-container--default .select2-selection--single .select2-selection__arrow b {
            margin-top: -5px !important;
        }
</style>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
<link href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" rel="stylesheet" />

<div class="questionnaire-modal" id="section2">
    @using (Ajax.BeginForm("_newDiabetesQuestionnaire", "Clinical", new AjaxOptions { HttpMethod = "POST", UpdateTargetId = "divPath", OnSuccess = "hideModal(); alert('Saved');" }, new { id = "newDiabetesQuestionnaire" }))
    {
        @Html.Hidden("taskId", (object)ViewBag.task)
        @Html.Hidden("DependantID", (object)ViewBag.DependantID)
        @Html.Hidden("id", (object)ViewBag.id)
        @Html.Hidden("template", (object)ViewBag.templateID)
        @Html.Hidden("pro", (object)ViewBag.programId)
        @Html.Hidden("questionnaireID", (object)ViewBag.questionnaireID)

        @Html.AntiForgeryToken()

        <div class="modal-header questionnaire-modal-header">
            <!--follow-up-button-->
            <div class="pull-right">
                <div class="diabetes-questionnaire-checkbox">
                    @Html.CheckBox("follow-up-all") <label for="follow-up-all" title="follow up - all"><span><i class="fas fa-circle follow-up-all"></i></span></label>
                </div>
            </div>
            <h2>&nbsp; DIABETES/SUIKERSIEKTE</h2>
        </div>

        <div class="form-horizontal" style="margin-left: 2%">
            <!-- diagnosis -->
            <div class="questionnaireContainer">
                <h3 class="questionnaire-heading">DIAGNOSIS/DIAGNOSE</h3>
                <!-- diagnosis-data -->
                <div id="diagnosisPanel_ci">
                    <hr class="questionnaire_Ruler" />
                    <br />
                    <div class="historyTable_header">HISTORY/GESKIEDENIS</div>
                    <div class="form-group">
                        <div class="row">
                            <table class="table historyTable_Quesionnaire table-hover" style="margin-left: 1.6%">
                                <thead>
                                    <tr>
                                        <th style="width: 18%">@Html.DisplayNameFor(model => model.PatientDiagnosis.CreatedDate)</th>
                                        <th style="width: 18%">@Html.DisplayNameFor(model => model.PatientDiagnosis.ProgramCode)</th>
                                        <th style="width: 18%">@Html.DisplayNameFor(model => model.PatientDiagnosis.ProgramDescription)</th>
                                        <th style="width: 18%">@Html.DisplayNameFor(model => model.PatientDiagnosis.EffectiveDate)</th>
                                        <th style="width: 18%">@Html.DisplayNameFor(model => model.PatientDiagnosis.CreatedBy)</th>
                                        <th style="width: 10%; text-align: center!important">@Html.DisplayNameFor(model => model.PatientDiagnosis.Comment)</th>
                                    </tr>
                                </thead>
                                @if (Model.programDiagnoses.Count() == 0)
                                {
                                    <tr><td colspan="6" style="text-align: center!important; color: rgba(128, 128, 128, 0.50)!important; font-size: 18px!important"><span><i class="fas fa-info-circle" style="color: rgba(128, 128, 128, 0.50)"></i></span> no records found</td></tr>
                                }
                                else
                                {
                                    foreach (var item in Model.programDiagnoses)
                                    {
                                        <tbody>
                                            @if (item.FollowUp == true)
                                            {
                                                <tr>
                                                    <td colspan="6" style="text-align: center!important; color: rgba(204, 0, 0, 0.70)!important; font-size: 18px!important"><span><i class="fas fa-info-circle" style="font-size: 18px; color: rgba(204, 0, 0, 0.70)!important"></i></span> Follow up required!</td>
                                                </tr>
                                            }
                                            else
                                            {
                                                <tr style="text-indent: 5px">
                                                    <td data-dateformat="DD-MM-YYYY">@Html.DisplayFor(Modelitem => item.CreatedDate)</td>
                                                    <td>@Html.DisplayFor(Modelitem => item.ProgramCode)</td>
                                                    <td>@Html.DisplayFor(Modelitem => item.ProgramDescription)</td>
                                                    <td data-dateformat="DD-MM-YYYY">@Html.DisplayFor(Modelitem => item.EffectiveDate)</td>
                                                    <td>@Html.DisplayFor(Modelitem => item.CreatedBy)</td>
                                                    <td style="text-align: center!important"><span class="@(!String.IsNullOrEmpty(item.Comment) ? "check-yes far fa-check-circle" : "check-no far fa-times-circle")" title="@item.Comment"></span></td>
                                                </tr>
                                            }
                                        </tbody>
                                    }
                                }
                            </table>
                        </div>
                    </div>
                    <hr class="questionnaire_Ruler" />
                    <br />
                    <!---->
                    <div class="form-group">
                        <div class="col-md-3" style="margin-left: 3.5%; text-align: right">
                            <p class="questionnaireQuestion">
                                When were you diagnosed with Diabetes?<br />
                                Wanneer was u gediagnoseer met Diabetes?
                            </p>
                        </div>
                        <div class="col-md-8">
                            <div>
                                <i class="fas fa-asterisk pulser asterisk-warning"></i>
                            </div>
                            <input class="text-box single-line input-validation-error infoBox_doctorQuestionnaire" data-val="true" data-val-date="The field must be a date." id="PatientDiagnosis.EffectiveDate" name="PatientDiagnosis.EffectiveDate" required type="date" aria-required="true" aria-describedby="DOB-error" aria-invalid="true" min="@DateTime.Now.ToString("1900-01-01")" max="@DateTime.Now.ToString("yyyy-MM-dd")" oninvalid="InvalidMsg(this);">
                            @Html.ValidationMessageFor(model => model.PatientDiagnosis.EffectiveDate, "", new { @class = "text-danger", @style = "font-size: 110%; margin-left: 1%" })
                        </div>
                        <br />
                        <hr class="settings-ruler" />
                        <div class="col-md-12">
                            <table class="table historyTable_Quesionnaire" style="margin-left: 0.8%">
                                <tbody>
                                    <tr style="background-color: #fff">
                                        <td class="socialHistory_Category" style="width: 20%">
                                            <p class="questionnaireQuestion">
                                                Where are you going to get your medication from?
                                            </p>
                                        </td>
                                        <td style="width: 50%">
                                            <select id="medication-registration" name="medication-registration" class="form-control select2" style="width: 45%;" onchange="MedicationRegistration()" required>
                                                <option subtext="" selected="selected" disabled>Select</option>
                                                <option value="Pharmacy" subtext=" ">Pharmacy</option>
                                                <option value="State" subtext=" ">State</option>
                                            </select>
                                            <br />@Html.ValidationMessage("medication-registration", "", new { @class = "text-danger", @style = "font-size: 95%; margin-left: 1%", @id = "nd-medication-registration-error" })
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <!-- medication -->
            <div class="questionnaireContainer">
                <div class="form-group pull-right">
                    <div class="col-md-4 diabetes-questionnaire-checkbox">
                        @Html.CheckBox("d-medication-ignore") <label for="d-medication-ignore" title="ignore"><span id="iMedical" class="ignore"><i class="fas fa-circle q-btn"></i></span></label>
                    </div>
                    <div class="col-md-4 diabetes-questionnaire-checkbox">
                        @Html.CheckBox("d-medication-followup") <label for="d-medication-followup" title="follow up"><span id="fMedical" class="follow-up"><i class="fas fa-circle q-btn"></i></span></label>
                    </div>
                </div>
                <h3 class="questionnaire-heading">MEDICATION/MEDIKASIE</h3>

                <!-- medication-data -->
                <div id="medicationpanel_diabetes">
                    <hr class="questionnaire_Ruler" />
                    <br />
                    <div class="historyTable_header">HISTORY/GESKIEDENIS</div>
                    <div class="form-group">
                        <div class="row">
                            <table class="table historyTable_Quesionnaire table-hover" id="tab_logic_diabetes_Medication" style="margin-left: 1.6%">
                                <thead>
                                    <tr>
                                        <th style="width: 8%; text-align: center!important">@Html.DisplayNameFor(model => model.generalScriptItems[0].scriptID)</th>
                                        <th style="width: 8%; text-align: center!important">@Html.DisplayNameFor(model => model.generalScriptItems[0].nappiCode)</th>
                                        <th style="width: 22%">@Html.DisplayNameFor(model => model.generalScriptItems[0].productName)</th>
                                        <th style="width: 5%; text-align: center !important">@Html.DisplayNameFor(model => model.generalScriptItems[0].quantity)</th>
                                        <th style="width: 22%">@Html.DisplayNameFor(model => model.generalScriptItems[0].directions)</th>
                                        <th style="width: 15%">@Html.DisplayNameFor(model => model.generalScriptItems[0].itemStatus)</th>
                                        <th style="width: 8%; text-align: center!important">@Html.DisplayNameFor(model => model.generalScriptItems[0].fromDate)</th>
                                        <th style="width: 8%; text-align: center!important">@Html.DisplayNameFor(model => model.generalScriptItems[0].toDate)</th>
                                    </tr>
                                </thead>
                                @if (Model.ScriptItems.Count() == 0)
                                {
                                    <tr><td colspan="9" style="text-align: center!important; color: rgba(128, 128, 128, 0.50)!important; font-size: 18px!important"><span><i class="fas fa-info-circle" style="color: rgba(128, 128, 128, 0.50)"></i></span> no records found</td></tr>
                                }
                                else
                                {
                                    foreach (var item in Model.ScriptItems)
                                    {
                                        <tbody>
                                            <tr style="text-indent: 5px">
                                                <td style="text-align: center!important">@Html.DisplayFor(Modelitem => item.scriptID)</td>
                                                <td style="text-align: center !important" class="checkQuest">@Html.DisplayFor(Modelitem => item.nappiCode)</td>
                                                <td>@Html.DisplayFor(Modelitem => item.productName)</td>
                                                <td style="text-align: center !important" data-dateformat="DD-MM-YYYY">@Html.DisplayFor(Modelitem => item.quantity)</td>
                                                <td>@Html.DisplayFor(Modelitem => item.directions)</td>
                                                <td>@Html.DisplayFor(Modelitem => item.itemStatus)</td>
                                                <td style="text-align: center !important" data-dateformat="DD-MM-YYYY">@Html.DisplayFor(Modelitem => item.fromDate)</td>
                                                <td style="text-align: center !important" data-dateformat="DD-MM-YYYY">@Html.DisplayFor(Modelitem => item.toDate)</td>
                                            </tr>
                                        </tbody>
                                    }
                                }
                            </table>
                        </div>
                    </div>
                    <div class="form-group">
                        <p style="color: rgba(8, 169, 250, 0.69); text-align: center; font-size: 16px">
                            <span><i class="fas fa-info-circle" style="color: rgba(8, 169, 250, 0.69)"></i></span> For additional medication, please go to "Patient Clinical" tab and add under the clinical section
                        </p>
                    </div>
                </div>
            </div>
            <!-- optometry -->
            <div class="questionnaireContainer">
                <div class="form-group pull-right">
                    <div class="col-md-4 diabetes-questionnaire-checkbox">
                        @Html.CheckBox("d-optometry-ignore") <label for="d-optometry-ignore" title="ignore"><span id="iOptometry" class="ignore"><i class="fas fa-circle q-btn"></i></span></label>
                    </div>
                    <div class="col-md-4 diabetes-questionnaire-checkbox">
                        @Html.CheckBox("d-optometry-followup") <label for="d-optometry-followup" title="follow up"><span id="fOptometry" class="follow-up"><i class="fas fa-circle q-btn"></i></span></label>
                    </div>
                </div>
                <h3 class="questionnaire-heading">OPTOMETRY/OPTOMETRIE</h3>

                <!-- optometry-data -->
                <div id="optometrypanel_diabetes">

                    <hr class="questionnaire_Ruler" />
                    <br />
                    <div class="historyTable_header">HISTORY/GESKIEDENIS</div>
                    <div class="form-group">
                        <div class="row">
                            <table class="table historyTable_Quesionnaire table-hover" id="socialHistoryTable" style="margin-left: 1.6%">
                                <thead>
                                    <tr>
                                        <th style="width: 16%">@Html.DisplayNameFor(model => model.vision.createdDate) <i class="fa fa-fw fa-sort"></i></th>
                                        <th style="width: 16%">@Html.DisplayNameFor(model => model.vision.vision) <i class="fa fa-fw fa-sort"></i></th>
                                        <th style="width: 16%">@Html.DisplayNameFor(model => model.vision.eyeTreatment) <i class="fa fa-fw fa-sort"></i></th>
                                        <th style="width: 16%">@Html.DisplayNameFor(model => model.vision.createdBy) <i class="fa fa-fw fa-sort"></i></th>
                                        <th style="width: 16%">@Html.DisplayNameFor(model => model.vision.programType) <i class="fa fa-fw fa-sort"></i></th>
                                        <th style="width: 16%">@Html.DisplayNameFor(model => model.vision.generalComments) <i class="fa fa-fw fa-sort"></i></th>
                                    </tr>
                                </thead>

                                @if (Model.visions.Count() == 0)
                                {
                                    <tr><td colspan="6" style="text-align: center!important; color: rgba(128, 128, 128, 0.50)!important; font-size: 18px!important"><span><i class="fas fa-info-circle" style="color: rgba(128, 128, 128, 0.50)"></i></span> no records found</td></tr>
                                }
                                else
                                {
                                    foreach (var item in Model.visions)
                                    {
                                        <tbody>
                                            @if (item.followUp == true)
                                            {
                                                <tr>
                                                    <td colspan="6" style="text-align: center!important; color: rgba(204, 0, 0, 0.70)!important; font-size: 18px!important"><span><i class="fas fa-info-circle" style="font-size: 18px; color: rgba(204, 0, 0, 0.70)!important"></i></span> Follow up required!</td>
                                                </tr>
                                            }
                                            else
                                            {
                                                <tr style="text-indent: 5px">
                                                    <td data-dateformat="DD-MM-YYYY">@Html.DisplayFor(Modelitem => item.createdDate)</td>
                                                    <td>@Html.Raw(Html.Encode(item.vision).Replace(",", "<br />" + ""))</td>
                                                    <td>@Html.Raw(Html.Encode(item.eyeTreatment).Replace(",", "<br />" + ""))</td>
                                                    <td data-dateformat="DD-MM-YYYY">@Html.DisplayFor(Modelitem => item.createdBy)</td>
                                                    <td>@Html.DisplayFor(Modelitem => item.programType)</td>

                                                    @if (item.generalComments == "None" || item.generalComments == "none" || item.generalComments == "NONE")
                                                    {
                                                        <td style="text-align: center!important"><span style="font-size: 14px; color: darkred; text-align: center!important"><i class="fas fa-times" title="No comment"></i></span></td>
                                                    }
                                                    else if (item.generalComments == "Follow up")
                                                    {
                                                        <td style="text-align: center!important; color: rgba(204, 0, 0, 0.70)!important; font-size: 18px!important"><span><i class="fas fa-info-circle" style="font-size: 18px; color: rgba(204, 0, 0, 0.70)!important"></i></span> Follow up!</td>
                                                    }
                                                    else if (item.generalComments == null || item.generalComments == " " || item.generalComments == "")
                                                    {
                                                        <td style="text-align: center!important"><span style="font-size: 14px; color: darkred; text-align: center!important"><i class="fas fa-times" title="No comment"></i></span></td>
                                                    }
                                                    else
                                                    {
                                                        <td style="text-align: center!important"><span style="font-size: 14px; color: forestgreen; text-align: center!important"><i class="fas fa-check" title="@item.generalComments"></i></span></td>
                                                    }
                                                </tr>
                                            }
                                        </tbody>
                                    }
                                }
                            </table>
                        </div>
                        <br />
                        <div class="form-group">
                            <div class="col-md-12" id="podiatry-question">
                                <table class="table historyTable_Quesionnaire" style="margin-left: 0.8%">
                                    <thead>
                                        <tr>
                                            <th style="width: 50%">
                                                Question
                                            </th>
                                            <th style="width: 50%">
                                                Action
                                            </th>
                                        </tr>
                                    </thead>
                                </table>
                                <table class="table historyTable_Quesionnaire" style="margin-left: 0.8%">
