                    <div class="acknowledgement-patientDisclaimer">
                        @if (Model.patientDisclaimers.Count() != 0)
                        {
                            foreach (var item in Model.patientDisclaimers)
                            {
                                if (item.SignedAcknowledgement == true)
                                {
                                    <div style="position: absolute; font-size: 300px; right: -6px; bottom: -62px; z-index: 100">
                                        <span><i class="fas fa-check-circle" style="color: rgba(3, 166, 120, 0.10)"></i></span>
                                    </div>
                                    <div class="pull-right" style="margin-right: 25px">
                                        <p class="acknowledgement-true">Disclaimer signed <span><i class="fas fa-check-circle" style="color: forestgreen"></i></span></p>
                                    </div>
                                    break;
                                }
                                else if (item.TelephonicAcknowledgement == true)
                                {
                                    <div style="position: absolute; font-size: 300px; right: -6px; bottom: -62px; z-index: 100">
                                        <span><i class="fas fa-check-circle" style="color: rgba(3, 166, 120, 0.10)"></i></span>
