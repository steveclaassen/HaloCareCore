                            @Html.CheckBox("Clinical_FollowUp", new { htmlAttributes = new { @class = "infoBox_doctorQuestionnaire" } })
                            <label for="Clinical_FollowUp" style="width: 50px"><i class="fas fa-star" style="font-size: 18px" title="follow up" id="Clinical_FollowUp"></i></label>
                        </div>
                    </div>
                </div>
                <div id="ClinicalPanel_ic">
                    <hr class="questionnaire_Ruler" />
                    <br />
                    <!--------------------------------------------------------------------------- history-table ------------------------------------------------------------------------>
                    <div class="historyTable_header">HISTORY/GESKIEDENIS</div>
                    <div class="form-group">
                        <div class="row">
                            <table class="table historyTable_Quesionnaire table-hover" id="tab_logic_CM_diab" style="margin-left: 1.6%">
                                <thead>
                                    <tr>
                                        <th width="25%">Created date</th>
                                        <th width="25%">Height/Lengte</th>
                                        <th width="25%">Weight/Gewig</th>
                                        <th width="25%">BMI</th>
                                    </tr>
                                </thead>
                                @if (Model.clinicalexams.Count() == 0)
                                {
                                    <tr><td colspan="4" style="text-align: center!important; color: rgba(128, 128, 128, 0.50)!important; font-size: 18px!important"><span><i class="fas fa-info-circle" style="color: rgba(128, 128, 128, 0.50)"></i></span> no records found</td></tr>
                                }
                                else
                                {
                                    foreach (var item in Model.clinicalexams)
                                    {
                                        <tbody>
                                            @if (item.followUp == true)
                                            {
                                                <tr>
                                                    <td colspan="4" style="text-align: center!important; color: rgba(204, 0, 0, 0.70)!important; font-size: 18px!important"><span><i class="fas fa-info-circle" style="font-size: 18px; color: rgba(204, 0, 0, 0.70)!important"></i></span> Follow up required!</td>
                                                </tr>
                                            }
                                            else
                                            {
                                                <tr style="text-indent: 5px">
                                                    <td data-dateformat="DD-MM-YYYY">@Html.DisplayFor(Modelitem => item.createdDate)</td>
                                                    <td>@Html.DisplayFor(Modelitem => item.height)</td>
                                                    <td>@Html.DisplayFor(Modelitem => item.weight)</td>
                                                    <td>@Html.DisplayFor(Modelitem => item.bmi)</td>
                                                </tr>
                                            }
                                        </tbody>
                                    }
                                }
                            </table>
                        </div>
                    </div>
                    <br />
                    <!------------------------------------------------------------------------ patient-history-new --------------------------------------------------------------------->
                    <div class="historyTable_header">PATIENT INFORMATION/PASIËNT INLIGTING</div>
                    <table class="table questionTable" id="tab_logic" style="margin-left: 1.6%">
                        <thead>
                            <tr>
                                <th style="width: 33%; background-color: none!important">Category</th>
                                <th style="width: 33%">Date</th>
                                <th style="width: 33%">Result</th>
                            </tr>
                        </thead>
                        <tbody class="arvHistory" id="historyTable">
                            <tr style="background-color: #fff">
