                            @if (Model.clinicalexams.Count() != 0)
                            {
                                @Html.CheckBox("Clinical_Confirmed", new { htmlAttributes = new { @class = "infoBox_doctorQuestionnaire" } })
                                <label for="Clinical_Confirmed" style="width: 50px"><i class="fas fa-chevron-circle-right" style="font-size: 18px" title="ignore" id="Clinical_Confirmed"></i></label>
                            }
                            else
                            {
                                @Html.CheckBox("Clinical_Confirmed", new { htmlAttributes = new { @class = "infoBox_doctorQuestionnaire" } })
                            }
                        </div>
                    </div>
                    <div class="col-md-4 checkBox_followUp">
