@using Microsoft.AspNetCore.Identity
<!-- navSlider -->
<div id="wrapper" class="">

    <div id="sidebar-wrapper_blueGray">

        <ul class="sidebar-nav" id="sidebar">
            <li class="space_dashboardSlider"></li>

            <li><a id="menu-toggle" href="#" title="Menu">Menu<span id="main_icon"></span><i class="sub_icon material-icons">&#xe871;</i></a></li>
            <!-- Dashboard -->
            <li><a href="@Url.Action("Index", "Home", new { })" style="border-bottom: 1px solid rgba(255, 255, 255, 0.05)" title="Home"><span>Home</span><span class="sub_icon fas fa-home" style="font-size: 14px"></span></a></li>

            <li data-toggle="collapse" data-target="#patientDash" class="collapsed">
                <a href="@Url.Action("Search", "Member", new { })" style="border-bottom: 1px solid rgba(255, 255, 255, 0.05)" title="Patient"><span>Patient</span><span class="sub_icon fa fa-user-circle" style="font-size: 14px"></span></a>
            </li>

            <!-- workflow -->
            <li data-toggle="collapse" data-target="#assignmentDrop" class="collapsed">
                @*<span class="fa-stack_Small has-badge" data-count="@ViewBag.assignment">*@
                <div class="badgeNotifications">
                    @ViewBag.assignment
                </div>

                <a href="@Url.Action("OpenAssignments", "Clinical", new {})" style="border-bottom: 1px solid rgba(255, 255, 255, 0.05)" title="Workflow"><span>Workflow</span><span class="sub_icon far fa-bell" style="font-size: 14px"></span></a>
                @*</span>*@
            </li>

            <!--Authorise -->
            <li data-toggle="collapse" data-target="#authorisations" class="collapsed">
                <a href="@Url.Action("OutstandingAuthorise", "Scripts", new {})" style="border-bottom: 1px solid rgba(255, 255, 255, 0.05)" title="Authorisations"><span>Authorisations</span><span class="sub_icon fas fa-check-circle" style="font-size: 14px"></span></a>
            </li>
            <!-- Clinical -->
            <li data-toggle="collapse" data-target="#reports" class="collapsed">
                <a href="@Url.Action("searchManagement", "patientManagement", new { DependentID = Context.Request.Query["DependentID"]})" style="border-bottom: 1px solid rgba(255, 255, 255, 0.05)" title="Search"><span>Search</span><span class="sub_icon fas fa-search" style="font-size: 14px"></span></a>
            </li>

            <li><a href="@Url.Action("Information", "Home", new { })" title="Information"><span>Information</span><span class="sub_icon fas fa-info-circle"></span></a></li>

            <li><a class="go_back" title="Back"><span style="font-size: 10px; position: fixed; bottom: 2px; margin-left: -18px">Back</span><span style="position:fixed; bottom: 15px; padding-left: 110px; margin-right: 0px" class="back_icon fas fa-arrow-alt-circle-left"></span></a></li>

        </ul>
    </div>
</div>

